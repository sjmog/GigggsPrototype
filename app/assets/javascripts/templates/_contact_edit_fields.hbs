
	

<div class="aside">
	<div class="control-group">
	<div class="inputWrap">
	  <div class="controls">
	  	
		{{view Ember.TextField valueBinding="name" id="name" placeholder="Gig Name" required="true"}}
		</div>
	  </div>
	</div>
	<div class="control-group" style="display:none">
	  <label class="control-label" for="lat">Latitude</label>
	  <div class="controls geoctrl">
		{{view Ember.TextField valueBinding="lat" id="lat" placeholder="Latitude" required="false" data-geo="lat"}}
	  </div>
	</div>
	<div class="control-group" style="display:none">
	  <label class="control-label" for="lng">Longitude</label>
	  <div class="controls geoctrl">
		{{view Ember.TextField valueBinding="lng" id="lng" placeholder="Longitude" required="false" data-geo="lng"}}
	  </div>
	</div>
	
	<div class="control-group">
	  <label class="control-label" for="location">Where?</label><br>
	  <div class="inputWrap">
	  <div class="controls">
		<input id="location" />
	</div>
	  </div>
	  <div class="maparea">
	  </div>
	</div>
	<div class="control-group">
		<label class="control-label" for="start">When do you start?</label><br>
		<div class="inputWrap">
		<div class="controls input-append date">
			{{view App.DatePicker valueBinding="App.ContactsNewController.start"}}
			<span class="add-on">
				<i class="icon-calendar"></i>
			</span>
		</div>
		</div>
		<div class="inputWrap" style="margin-top:4px;">
		<div class="controls" id="datepickerTimeTooltip">
			{{view Ember.TextField id="start_time"}}
		</div>
		</div>
	</div>
	<div class="control-group">
		<label class="control-label" for="start">When do you finish?</label><br>
		<div class="controls input-append date">
			{{view App.DatePicker valueBinding="App.ContactsNewController.end"}}
			<span class="add-on">
				<i class="icon-calendar"></i>
			</span>
		</div>
		<div class="controls" id="datepickerTimeTooltip2">
			{{view Ember.TextField id="end_time"}}
		</div>
	</div>
	<div class="control-group">
	  <label class="control-label" for="cat1">Category</label><br>
	  <div class="controls">
	
	    <!--v| These are jquery-driven category selection views |v-->
	
	    {{#view App.CategoriesView}}
		{{partial categories}}
		{{/view}}<br>
		
			  </div>
	</div>
	<!-- here's the subgroup. It contains less important gig properties, so it is hidden by default. Users can expand it by clicking on the 'Add more Detail...' link below the creation screen (but above the email which is critical for signup). Add {{bindAttr class="deets:notHere:here"}} later on -->
	
	{{#if isExpanded}}
  

	<div id="subgroup">
		<div class="control-group">
		  <label class="control-label" for="pricelower">Lowest entry fee</label><br>
		  <div class="controls">
			{{view Ember.TextField valueBinding="pricelower" id="pricelower" placeholder="The lowest entry fee (can be free)" required="false"}}
		  </div>
		</div>
		<div class="control-group">
		  <label class="control-label" for="pricehigher">Highest entry fee</label><br>
		  <div class="controls">
			{{view Ember.TextField valueBinding="pricehigher" id="pricehigher" placeholder="The highest entry fee (can be free)" required="false"}}
		  </div>
		</div>
		<div class="control-group">
		  <label class="control-label" for="external_url">Gig website</label><br>
		  <div class="controls">
			{{view Ember.TextField valueBinding="external_url" id="external_url" placeholder="The site for your Gig (if you have one)" required="false"}}
		  </div>
		</div>
		<div class="control-group">
		  <label class="control-label" for="ticket_url">Tickets website</label><br>
		  <div class="controls">
			{{view Ember.TextField valueBinding="ticket_url" id="ticket_url" placeholder="The site to buy tickets (if you have one)" required="false"}}
		  </div>
		</div>
		<div class="control-group">
		  <label class="control-label" for="notes">About</label><br>
		  <div class="controls">
			{{view Ember.TextArea valueBinding="notes" id="notes" placeholder="Notes"}}
		  </div>
		</div>
		<div class="control-group">
		  <label class="control-label" for="phone">Media</label><br>
		  <div class="controls">
			{{#each phoneNumbers}}
				  <div class="phone-number">
				{{view Ember.TextField valueBinding="number" placeholder="Your contact number"}}
				<a class="btn" {{action removePhoneNumber this}}><i class="icon-minus-sign"></i> Remove</a>
			  </div>
			{{/each}}
			<a class="btn" {{action addPhoneNumber}}><i class="icon-plus-sign"></i> Add media</a>
		  </div>
		</div>
		</div>
		{{else}}
  <button {{action "expand" contacts_new}}>Show More</button>
{{/if}}
	<div class="control-group">
		
		<div class="controls">
			{{view Ember.TextField valueBinding="email" id="email" data-toggle="tooltip" rel="tooltip" type="email" placeholder="Sign-up e-mail"}}
		</div>
	</div>
</div>

